{
    "manifest_version": 3,
    "name": "Weather Forecast",
    "version": "1.0",
    "description": "My first Extension",
    "icons": {
        "16" : "/images/m_16.png",
        "128" : "/images/m_128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.com/*"],
          "css": ["/style/main.css"],
          "js": ["/js/main.js"]
        }
      ],
      "permissions": ["storage"],
      "action": {
        "default_popup": "popup.html"
      }
}